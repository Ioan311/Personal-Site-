<form id="form_inreg"  method="post" class="date"  action="<%= locals.action %>"  enctype="multipart/form-data">
    <p>
        <label>
            Username: <input type="text" name="username" id="inp-username" value="prof<%= Math.floor(Math.random()*100000) %>" required pattern="[A-Za-z0-9]+">
        </label>
    </p>
    <p>
        <label>
            First Name: <input type="text" name="first_name" id="inp-nume"  value="Gogulescu" >
        </label>
    </p>
    <p>
        <label>
            Last Name: <input type="text" name="last_name" id="inp-prenume" value="Gogu"  >
        </label>
    </p>
    <p>
        <label>
            Password: <input id="parl" type="password" required  name="password" value="">
        </label>
    </p>
    <p id="p-reintrod">
        <label >
            Confirm password: <input id="rparl" required type="password" name="rparola" value="">
        </label>
    </p>
    <p>
        <label>
            Email: <input type="text" name="email" id="inp-email" required value="profprofprof007@gmail.com"  size="50">
        </label>
    </p>
    <p>
        <label>
            Chat text color: <select name="culoare_chat" id="sel-culoare"><option value="red" selected>red</option><option value="green">green</option><option value="blue">blue</option></select>
        </label>
    </p>
    <p>
        <label>
            Profile Photo: <input type="file" name="poza">
        </label>			
    </p>
    <p>		
        <input type="submit" value="Submit">
        <input type="reset" value="Reset">
    </p>
</form>